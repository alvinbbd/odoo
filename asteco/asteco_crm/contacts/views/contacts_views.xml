<odoo>
    <data>

        <record id="log_phone_call_wizard_form_view" model="ir.ui.view">
            <field name="name">Log Phone Call</field>
            <field name="model">log.phone.call.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <group>
                            <field name="date_and_time"/>
                        </group>
                    </group>
                    <group>
                        <field name="notes"/>
                    </group>
                    <group invisible="1">
                        <field name="contact_id" invisible="1"/>
                        <field name="listing_id" invisible="1"/>
                        <field name="lead_id" invisible="1"/>
                        <field name="deal_id" invisible="1"/>
                    </group>
                    <footer>
                        <button name="save" string="Save" type="object" class="oe_asteco_default"/>
                        <button string="Cancel" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="atk_social_media_view_form" model="ir.ui.view">
            <field name="name">Social Media Form View</field>
            <field name="model">contacts.social.media</field>
            <field name="arch" type="xml">
                <form create="0" all_invoices="0">
                    <header>
                        <button name="sms.send_sms_action" type="action" string="Send SMS"/>
                    </header>
                    <script>
                        $(document).ready(function(){
                            $('span:eq(0)').html("Add New");
                            $( "input.o_required_modifier:text" ).each(function() {
                            $(this).siblings().attr("class", "so-mandtry-star");
                            });
                            $( "select.o_required_modifier" ).each(function() {
                            $(this).siblings().attr("class", "so-mandtry-star");
                            });
                            var contact_type_id = document.getElementsByName("contact_type");
                            $(contact_type_id).siblings().attr("class", "so-mandtry-star");
                            $("button[class='btn btn-sm btn-primary']:eq(1)").hide();
                        });
                    </script>
                    <group>
                        <group>
                            <field name="image" invisible="1"/>
                            <field name="type" />
                            <field name="partner_id" invisible="1"/>
                        </group>
                        <group>
                            <field name="url" />
                        </group>
                    </group>
                </form>
            </field>
        </record>

