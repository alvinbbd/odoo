                                                <div class="col-xs-12" style="padding-top:10px;">
                                                    <field name="document_ids" widget="many2many_binary" string="Upload" class="ast-doc-attach"/>
                                                </div>
                                            </div>
                                            </page>

                                    </notebook>
                                </group>
                            </div>
                            <div class="col-xs-12" style="padding:15px;">
	                            <div class="col-md-4 col-xs-12 text-center">
	                                <b>Created Date &amp; Time : </b><field class="ast-create-date" name="create_date" readonly="True"/>
	                            </div>
	                            <div class="col-md-4 col-xs-12 text-center">
	                                <b>Last Updated Date &amp; Time : </b><field class="ast-create-date" name="write_date" readonly="True"/>
	                            </div>
	                            <div class="col-md-4 col-xs-12 text-center">
	                                <b>Last Updated By : </b><field name="write_uid" readonly="True"/>
	                            </div>
                            </div>
                        </div>
                    </sheet>
                    <div class="oe_chatter">
                        <field name="message_follower_ids" widget="mail_followers"/>
                        <field name="activity_ids" widget="mail_activity"/>
                        <field name="message_ids" widget="mail_thread"/>
                    </div>
                </form>
            </field>
        </record>


        <record id="atk_contacts_contacts_view_tree" model="ir.ui.view">
                <field name="name">Contacts Tree View</field>
                <field name="model">res.partner</field>
                <field name="arch" type="xml">
                    <tree duplicate="0" delete="0" default_order='create_date desc' >
                        <field name="is_contact" invisible="1"/>
                        <field name="ref_name"/>
                        <field name="name"/>
                        <field name="mobile"/>
                        <field name="email"/>
                        <field name="head" string="Contact Type"/>
                        <field name="source"/>
                        <field name="create_uid"/>
                        <field name="create_date"/>
                    </tree>
                </field>
        </record>


        <record id="contacts_kanban_view" model="ir.ui.view">
            <field name="name">Contacts Kanban View</field>
            <field name="model">res.partner</field>
<!--            <field name="type">kanban</field>-->
            <field name="arch" type="xml">
                <kanban>
                    <field name="name"/>
                    <templates>
                        <t t-name="kanban-box">
                            <div>
                                <div>
                                    <field name="name"/>
                                </div>
                            </div>
                        </t>
                    </templates>
                </kanban>
            </field>
        </record>

        <record id="contact_filter" model="ir.ui.view">
            <field name="name">Contact Filter</field>
            <field name="model">res.partner</field>
            <field name="arch" type="xml">
                <search string="Search Contacts">
                    <field name="name" string="My Contacts" filter_domain="['|',('email','ilike',self),'|',('mobile','ilike',self),'|',('ref_name','ilike',self),('name','ilike',self)]"/>
                    <!-- <field name="name" string="My Contacts" /> -->
                    <separator/>
                    <filter string="My Contacts" name="my_contact" domain="['|',('create_uid','=',uid), ('assigned_to_id.user_id','=',uid)]"/>
                    <filter string="Linked Contacts" name="linked_ones" domain="[('is_link_to_contact','=',True)]" help="..."/>
                    <filter string="All Contacts" name="contact_all" domain="[('create_uid','=',uid)]"/>
                    <separator/>
                </search>
            </field>
        </record>


        <record id="atk_contacts_contacts_Action" model="ir.actions.act_window">
            <field name="name">Contacts</field>
            <field name="res_model">res.partner</field>
            <field name="view_mode">tree,form,kanban</field>
            <field name="view_ids" eval="[(5, 0, 0),
                (0, 0, {'view_mode': 'tree', 'view_id': ref('atk_contacts_contacts_view_tree')}),
                (0, 0, {'view_mode': 'form', 'view_id': ref('atk_contacts_view_form')})]"/>
            <field name="domain">[('is_contact','=',True)]</field>
            <field name="context">{'default_is_contact': True, 'search_default_my_contact':uid}</field>
            <field name="help">You don't have any contacts.</field>

        </record>

        <menuitem id="contacts_root_menu" name="Contacts" sequence="1" parent="interact_root_menu" action="atk_contacts_contacts_Action"/>

        <act_window
            id="account.action_all_partner_invoices"
            groups="account.group_account_manager"
            name="All Invoices"
            res_model="account.invoice"
            domain="[('partner_id', 'child_of', active_id)]"
            src_model="res.partner"/>
    </data>
</odoo>
