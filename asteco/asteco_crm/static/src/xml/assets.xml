<odoo>
	<data>
		<template id="asteco_crm_assets_backend" inherit_id="web.assets_backend" name="CRM Asteco Assets Backend">
	        <xpath expr="." position="inside">
				<script type="text/javascript" src="/asteco_crm/static/src/js/custom.js"></script>
	        </xpath>
	        <xpath expr="//script[@src='/web/static/src/js/fields/relational_fields.js']" position="replace">
				<script type="text/javascript" src="/asteco_crm/static/src/js/relational_fields.js"></script>
	        </xpath>
	    </template>

	    <template id="asteco_assets_backend" inherit_id="web_editor.assets_backend" name="Asteco Web Editor Backend Assets (used in backend interface)">
	    	<xpath expr="//link[@href='/web_editor/static/src/less/web_editor.backend.less']" position="replace">
	    		<link rel="stylesheet" type="text/less" href="/asteco_crm/static/src/less/web_editor.backend.less"/>	
	    	</xpath>
	    </template>
	</data>
</odoo>
